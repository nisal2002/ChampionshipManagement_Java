<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="430.0" prefWidth="600.0" styleClass="backgroundColor" stylesheets="@style.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.rallycrosschampionship.VctController">
   <children>
      <Label layoutX="174.0" layoutY="44.0" text="Viewing the rally cross standings table">
         <font>
            <Font size="15.0" />
         </font></Label>
      <Button fx:id="btnhome" layoutX="27.0" layoutY="382.0" mnemonicParsing="false" onAction="#backToHome" prefWidth="91.0" text="HOME" />
      <Button fx:id="btnexit" layoutX="480.0" layoutY="382.0" mnemonicParsing="false" onAction="#onExit" prefWidth="91.0" text="EXIT" />
      <Button fx:id="vctview" layoutX="254.0" layoutY="81.0" mnemonicParsing="false" onAction="#onViewVct" prefWidth="91.0" text="VIEW" />
      <TableView fx:id="vcttable" layoutX="27.0" layoutY="126.0" prefHeight="200.0" prefWidth="545.0">
        <columns>
            <TableColumn fx:id="colplace" prefWidth="75.0" text="Place" />
          <TableColumn fx:id="colfname" prefWidth="75.0" text="First Name" />
          <TableColumn fx:id="collname" prefWidth="75.0" text="Last Name" />
            <TableColumn fx:id="colage" prefWidth="75.0" text="Age" />
            <TableColumn fx:id="colteam" prefWidth="75.0" text="Team" />
            <TableColumn fx:id="colcar" prefWidth="75.0" text="Car" />
            <TableColumn fx:id="colpoints" prefWidth="90.6666259765625" text="Current Points" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
